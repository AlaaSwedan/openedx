	padding-right: 30px;
	padding-bottom: 10px;
}
.video-content::before{
	position: absolute;
	content: "";
	right: 0;
	left: 0;
	bottom: 0;
	top: 0;
	background-color: rgba(54,53,51,0.95);
}
.video-content h3 {
	font-size: 16px;
	color: #fff;
	letter-spacing: -0.32px;
	line-height: 26px;
